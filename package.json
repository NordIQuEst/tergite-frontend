{
	"name": "tergite-landing-page",
	"version": "0.0.1",
	"private": true,
	"proxy": "http://localhost:8002",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --write \"{src,cypress}/**/*.{js,jsx,ts,tsx}\"",
		"mocks": "mocks-server",
		"mocks:no-cli": "mocks-server --no-plugins.inquirerCli.enabled",
		"start-test": "NODE_ENV=test next start",
		"mocks:no-cli_and_start:app": "start-server-and-test mocks:no-cli http://0.0.0.0:8002 -- \"start-test\"",
		"build-and-start-test-server": "NODE_ENV=test npm run build && start-server-and-test mocks:no-cli_and_start:app http://0.0.0.0:3000",
		"e2e": "npm run build-and-start-test-server -- \"cypress run\"",
		"visual-e2e": "npm run build-and-start-test-server -- \"cypress open\""
	},
	"dependencies": {
		"@types/node": "20.5.4",
		"@types/react": "18.2.21",
		"@types/react-dom": "18.2.7",
		"autoprefixer": "10.4.15",
		"jose": "^4.15.4",
		"next": "13.4.19",
		"postcss": "8.4.28",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"swr": "^2.2.4",
		"tailwindcss": "3.3.3",
		"typescript": "5.1.6"
	},
	"devDependencies": {
		"@mocks-server/cypress-commands": "^6.1.1",
		"@mocks-server/main": "^4.1.0",
		"@types/jsonwebtoken": "^9.0.4",
		"cookie-parser": "^1.4.6",
		"cypress": "^13.3.2",
		"eslint": "8.47.0",
		"eslint-config-next": "13.4.19",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-cypress": "^2.15.1",
		"prettier": "^3.0.2",
		"serve": "^14.2.1",
		"start-server-and-test": "^2.0.0"
	}
}

