{
	"private": true,
	"version": "2024.02.0",
	"scripts": {
		"postinstall": "node -e \"process.env.NODE_ENV == 'production' && process.exit()\" || husky install",
		"prepare": "node -e \"process.env.NODE_ENV == 'production' && process.exit()\" || husky install",
		"dev": "next -p 3000",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"test-start": "NODE_ENV=test next start",
		"test-dev": "NODE_ENV=test next -p 3000",
		"start-test-server": "NODE_ENV=test npm run build && start-server-and-test test-start http://0.0.0.0:3000",
		"start-test-dev-server": "NODE_ENV=test npm run build && start-server-and-test test-dev http://0.0.0.0:3000",
		"e2e": "npm run start-test-server -- \"cypress run\"",
		"visual-e2e": "npm run start-test-dev-server -- \"cypress open\"",
		"format": "prettier --write \"{src,cypress}/**/*.{js,jsx,ts,tsx}\"",
		"pre-commit": "npm run format &&  next lint"
	},
